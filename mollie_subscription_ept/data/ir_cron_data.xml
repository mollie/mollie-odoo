<?xml version="1.0" encoding="UTF-8" ?>
<odoo>
    <record id="ir_cron_update_mollie_payments" model="ir.cron">
        <field name='name'>Update Mollie Payments</field>
        <field name='interval_number'>05</field>
        <field name='interval_type'>minutes</field>
        <field name="numbercall">-1</field>
        <field name="active" eval="True" />
        <field name="doall" eval="False" />
        <field name="code">model.auto_update_payments()</field>
        <field name="state">code</field>
        <field name="model_id" ref="model_mollie_payment" />
    </record>

    <record id="ir_cron_update_mollie_subscription" model="ir.cron">
        <field name='name'>Update Mollie Subscriptions</field>
        <field name='interval_number'>15</field>
        <field name='interval_type'>minutes</field>
        <field name="numbercall">-1</field>
        <field name="active" eval="True" />
        <field name="doall" eval="False" />
        <field name="code">model.auto_update_subscription()</field>
        <field name="state">code</field>
        <field name="model_id" ref="model_mollie_subscription" />
    </record>
</odoo>
